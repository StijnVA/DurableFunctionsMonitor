{
	"name": "durablefunctionsmonitor.vscodeext",
	"displayName": "Durable Functions Monitor",
	"description": "Durable Functions Monitor - a monitoring/debugging UI tool for Azure Durable Functions",
	"version": "1.2.0",
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.durableFunctionsMonitor"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.durableFunctionsMonitor",
				"title": "Durable Functions Monitor"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.39.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.2.0"
	},
	"dependencies": {
		"axios": "^0.19.0",
		"portscanner": "^2.2.0",
		"tree-kill": "^1.2.1"
	}
}
